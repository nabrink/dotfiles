[user]
  name = Sebastian Nabrink
	email = sebastian.nabrink@hotmail.com

[credential]
	helper = osxkeychain

[push]
  default = simple

[alias]
  g = log --all --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  d = diff --patch-with-stat
  c = clone --recursive
  ci = commit
  co = checkout
  go = checkout -B
  tags = tag -l
  remotes = remote -v
  undopush = push -f origin HEAD^:master
  pom = push origin master
  s = status -sb
  st = status
  branches = branch -a

[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[merge]
  log = true
[help]
  autocorret = 2500

[branch "master"]
  remote = origin
  merge = refs/heads/master

[pull]
  rebase = true
